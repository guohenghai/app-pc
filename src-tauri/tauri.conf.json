{
	"build": {
		"beforeDevCommand": "pnpm dev",
		"beforeBuildCommand": "pnpm build",
		"devPath": "http://localhost:1420",
		"distDir": "../dist"
	},
	"package": {
		"productName": "apptest",
		"version": "0.1.1"
	},
	"tauri": {
		"allowlist": {
			"all": false,
			"http": {
				"scope": ["http://**", "https://**"]
			},
			"shell": {
				"all": false,
				"open": true
			}
		},
		"bundle": {
			"active": true,
			"targets": "all",
			"identifier": "com.cssca.app",
			"icon": ["icons/32x32.png", "icons/128x128.png", "icons/128x128@2x.png", "icons/icon.icns", "icons/icon.ico"]
		},
		"security": {
			"csp": null
		},
		"updater": {
			"active": true,
			"dialog": true,
			"endpoints": ["https://guohenghai.github.io/app-pc/update.json"],
			"pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDlEQTM0NUM3REZDM0M3RkIKUldUN3g4UGZ4MFdqbmR0RlpxbnlpanRoM1lMdUNXNWoyQ096VEo2Ry9oUzVZalg1QjdkVnozcjQK"
		},
		"windows": [
			{
				"fullscreen": false,
				"resizable": true,
				"title": "apptest",
				"width": 800,
				"height": 600
			}
		]
	}
}
